{
  "name": "reduce",
  "version": "0.0.1",
  "description": "A command line wrapper for reduce-css and reduce-js",
  "main": "lib/main",
  "bin": {
    "reduce": "bin/reduce-cli.js"
  },
  "scripts": {
    "test": "npm run lint && task-tape test/*.js | tap-summary",
    "lint": "eslint *.js lib/**/*.js test/*.js bin/*.js",
    "coverage": "istanbul cover -i 'lib/**/*.js' -i '*.js' --print both task-tape -- test/*.js",
    "check-coverage": "istanbul check-coverage --statements 90 --functions 90 --branches 85 --lines 90",
    "upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "cover": "npm run coverage && npm run check-coverage && npm run upload-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/reduce-cli.git"
  },
  "keywords": [
    "browserify",
    "cli"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/reduce-cli/issues"
  },
  "homepage": "https://github.com/zoubin/reduce-cli#readme",
  "dependencies": {
    "callback-sequence": "^1.2.1",
    "commander": "^2.9.0",
    "reduce-css": "^1.0.3",
    "reduce-css-postcss": "^1.0.5",
    "reduce-js": "^3.0.1",
    "util-mix": "^3.0.2"
  },
  "devDependencies": {
    "del": "^2.0.2",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.1",
    "istanbul": "^0.4.0",
    "tap-summary": "^1.0.0",
    "tape": "^4.2.0",
    "task-tape": "^1.0.0"
  }
}
